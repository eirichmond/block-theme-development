{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#195641",
					"name": "Dark Green",
					"slug": "dark-green"
				},
				{
					"color": "#4aa775",
					"name": "Light Green",
					"slug": "light-green"
				},
				{
					"color": "#363636",
					"name": "Charcoal",
					"slug": "charcoal"
				},
				{
					"color": "#2d2d2d",
					"name": "Dark Charcoal",
					"slug": "dark-charcoal"
				}
			]
		},
		"layout": {
			"contentSize": "1100px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": ["file:./assets/fonts/roboto_normal_300.ttf"]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": ["file:./assets/fonts/roboto_normal_500.ttf"]
						},
						{
							"fontFamily": "Roboto",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/roboto_normal_700.ttf"]
						}
					],
					"fontFamily": "Roboto",
					"slug": "roboto"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.9rem",
					"slug": "small"
				},
				{
					"fluid": false,
					"name": "Medium",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.85rem",
						"min": "1.39rem"
					},
					"name": "Large",
					"size": "1.85rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2.5rem",
						"min": "1.85rem"
					},
					"name": "Extra Large",
					"size": "1.95rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "2.8rem",
						"min": "2.5rem"
					},
					"name": "XX Large",
					"size": "2.8rem",
					"slug": "xx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"bottom": {
								"style": "solid",
								"width": "1px"
							},
							"color": null,
							"left": {
								"style": "solid",
								"width": "1px"
							},
							"radius": "4px",
							"right": {
								"style": "solid",
								"width": "1px"
							},
							"style": null,
							"top": {
								"style": "solid",
								"width": "1px"
							},
							"width": null
						},
						"spacing": {
							"padding": {
								"bottom": "var(--wp--preset--spacing--30)",
								"left": "var(--wp--preset--spacing--50)",
								"right": "var(--wp--preset--spacing--50)",
								"top": "var(--wp--preset--spacing--30)"
							}
						},
						"typography": {
							"fontSize": "11px",
							"letterSpacing": "1px",
							"textTransform": "uppercase"
						}
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "14px",
					"fontStyle": "normal",
					"fontWeight": "500",
					"textTransform": "uppercase"
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": {
						"bottom": "14px",
						"top": "14px"
					}
				},
				"typography": {
					"fontSize": "18px",
					"fontStyle": "normal",
					"fontWeight": "300",
					"lineHeight": "30px"
				}
			}
		},
		"elements": {
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"textTransform": "uppercase"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"textTransform": "uppercase"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			}
		},
		"spacing": {
			"blockGap": "0"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2
}
